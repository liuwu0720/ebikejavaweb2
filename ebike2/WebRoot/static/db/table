-- Create table
create table DDC_FLOW
(
  id                NUMBER not null,
  lsh               VARCHAR2(20) not null,
  ywlx              VARCHAR2(20),
  ywyy              VARCHAR2(20),
  hyxhzh            VARCHAR2(20),
  dabh              VARCHAR2(20),
  cphm              VARCHAR2(20),
  ppxh              VARCHAR2(20),
  cysy              VARCHAR2(20),
  djh               VARCHAR2(30),
  jtzz              VARCHAR2(20),
  jsrxm1            VARCHAR2(20),
  xb1               VARCHAR2(20),
  sfzmhm1           VARCHAR2(20),
  lxdh1             VARCHAR2(20),
  jsrxm2            VARCHAR2(20),
  xb2               VARCHAR2(20),
  sfzmhm2           VARCHAR2(20),
  lxdh2             VARCHAR2(20),
  jsrxm3            VARCHAR2(20),
  xb3               VARCHAR2(20),
  sfzmhm3           VARCHAR2(20),
  lxdh3             VARCHAR2(20),
  xsqy              VARCHAR2(20),
  bz                VARCHAR2(500),
  slzl              VARCHAR2(100),
  slyj              VARCHAR2(20),
  slbz              VARCHAR2(100),
  slr               VARCHAR2(20),
  slrq              DATE,
  slbm              VARCHAR2(20),
  gdyj              VARCHAR2(20),
  tbyy              VARCHAR2(100),
  gdbz              VARCHAR2(100),
  gdr               VARCHAR2(20),
  gdrq              DATE,
  gdbm              VARCHAR2(20),
  syn_flag          VARCHAR2(10),
  tran_flag         VARCHAR2(10),
  tran_date         DATE,
  yclb              VARCHAR2(20),
  vc_user1img       VARCHAR2(100),
  vc_user2img       VARCHAR2(100),
  vc_ebike_img      VARCHAR2(100),
  vc_user1_cardimg1 VARCHAR2(100),
  vc_user1_cardimg2 VARCHAR2(100),
  vc_user2_cardimg1 VARCHAR2(100),
  vc_user2_cardimg2 VARCHAR2(100),
  vc_ebike_invoice  VARCHAR2(100),
  ssdwid            VARCHAR2(200),
  tablename         VARCHAR2(20),
  tableid           NUMBER,
  sl_index          NUMBER
)
tablespace EBIKEOUT_DATA
  pctfree 10
  initrans 1
  maxtrans 255
  storage
  (
    initial 64K
    minextents 1
    maxextents unlimited
  );
