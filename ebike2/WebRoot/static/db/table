  cjbm      VARCHAR2(20),
  hyxhlb    VARCHAR2(10),
  syn_flag  VARCHAR2(10),
  tran_flag VARCHAR2(10),
  tran_date DATE,
  totalpe   NUMBER,
  nenable   NUMBER default 1
)
